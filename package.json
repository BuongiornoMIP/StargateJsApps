    "gulp-watch": "^4.3.5",
    "gulp-webserver": "^0.9.1",
    "istanbul": "^0.4.1",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.3.4",
    "jasmine-reporters": "^2.0.7",
    "jsdoc-to-markdown": "^1.3.3",
    "jshint": "~2.8.0",
