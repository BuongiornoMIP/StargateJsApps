  "version": "0.4.4",
  "description": "StargateJS for HTML5 apps",
  "main": "dist/stargate.js",
  "scripts": {
    "preversion": "npm test",
    "postversion": "git checkout gh-pages && git add -A gh-pages/ && git commit -m 'Updated' && git push origin gh-pages && git checkout master",
    "version": "gulp build && npm run docs",
    "clean": "rm -r gh-pages",
    "docs": "node bin/generate_docs",
    "test": "gulp lint && gulp karma:singlerun --browsers=PhantomJS --reporters=mocha,coverage,coveralls"
  },
  "bin": {
    "docs": "bin/generate_docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BuongiornoMIP/StargateJsApps.git"
  },
  "keywords": [
    "cordova",
    "hybrid",
    "html5",
    "apps"
  ],
  "author": "Buongiorno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BuongiornoMIP/StargateJsApps/issues"
  },
  "homepage": "https://github.com/BuongiornoMIP/StargateJsApps#readme",
  "devDependencies": {
    "bower": "^1.6.5",
    "colors": "^1.1.2",
